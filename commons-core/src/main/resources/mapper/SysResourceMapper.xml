        <result column="RESOURCE_TYPE" property="resourceType" jdbcType="VARCHAR"/>
        <result column="RESOURCE_CODE" property="resourceCode" jdbcType="VARCHAR"/>
        <result column="RESOURCE_URL" property="resourceUrl" jdbcType="VARCHAR"/>
